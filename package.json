{
  "name": "es-file-traverse",
  "version": "0.6.0",
  "description": "Facilities for traversing JavaScript files",
  "author": "Brett Zamir",
  "contributors": [],
  "scripts": {
    "prepublishOnly": "run-s license-badges",
    "license-badge": "license-badger --corrections --uncategorizedLicenseTemplate \"\\${license} (\\${name} (\\${version}))\" --filteredTypes=nonempty --textTemplate \"License types\n(project, deps, and bundled devDeps)\" --packageJson --production badges/licenses-badge.svg",
    "license-badge-dev": "license-badger --corrections --filteredTypes=nonempty --textTemplate \"License types\n(all devDeps)\" --allDevelopment badges/licenses-badge-dev.svg",
    "license-badges": "run-s license-badge license-badge-dev",
    "build-cli-svg": "clp --format=svg --config=src/optionDefinitions.js doc-includes/cli.svg",
    "build-cli": "npm run build-cli-svg",
    "cli": "node ./bin/cli.js",
    "help": "node ./bin/cli.js --help",
    "version": "node ./bin/cli.js --version",
    "cli-example": "npm run cli -- --file=./test/fixtures/main.js",
    "eslint-badge": "eslint-formatter-badger --eslintCache --outputPath badges/eslint-badge.svg .",
    "eslint-3rdparty-badge": "eslint-formatter-badger --eslintCache --mainTemplate=\"ESLint 3rd party light audit (\\${ruleMapCount} rules against \\${numFiles} files)\" --filteredTypes intrusive,vulnerability --ruleMap .eslintRuleTypeMap.json --outputPath badges/eslint-thirdparty.svg --noEslintInlineConfig --noUseEslintIgnore --noUseEslintrc --eslintConfigPath .eslintrc-3rdparty.js `node ./bin/cli.js --file ./bin/cli.js --node --cjs`",
    "eslint-badges": "npm run eslint-badge && npm run eslint-3rdparty-badge",
    "eslint-3rdparty": "eslint --cache --no-inline-config --no-ignore --no-eslintrc --config .eslintrc-3rdparty.js `node ./bin/cli.js --file ./bin/cli.js --node --cjs`",
    "eslint": "eslint --cache --ext=js,md,html .",
    "lint": "npm run eslint",
    "coverage-badge": "coveradge badges/coverage-badge.svg",
    "mocha": "mocha",
    "nyc": "echo 'Removing esm cache which seems to blank out istanbul coverage when breaking out early' && rm -Rf node_modules/.cache && nyc npm run mocha && npm run coverage-badge",
    "test": "npm run eslint && npm run nyc"
  },
  "engines": {
    "node": ">=10.4.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "json-summary",
      "text"
    ]
  },
  "repository": "https://github.com/brettz9/es-file-traverse",
  "license": "MIT",
  "keywords": [
    "eslint",
    "files"
  ],
  "main": "./src/index.js",
  "bin": {
    "es-file-traverse": "./bin/cli.js"
  },
  "homepage": "https://github.com/brettz9/es-file-traverse",
  "bugs": "https://github.com/brettz9/es-file-traverse/issues",
  "dependencies": {
    "array-flat-polyfill": "^1.0.1",
    "babel-eslint": "^10.1.0",
    "builtin-modules": "^3.1.0",
    "command-line-basics": "^0.8.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "esquery": "^1.3.1",
    "file-fetch": "^1.4.1",
    "find-package-json": "^1.2.0",
    "globby": "^11.0.1",
    "htmlparser2": "^4.1.0",
    "resolve": "https://github.com/brettz9/resolve#pkg-json-module"
  },
  "devDependencies": {
    "@fintechstudios/eslint-plugin-chai-as-promised": "^3.0.2",
    "@mysticatea/eslint-plugin": "^13.0.0",
    "@typescript-eslint/parser": "^3.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "command-line-publish": "^0.7.0",
    "coveradge": "^0.6.0",
    "cypress-multi-reporters": "^1.4.0",
    "eslint": "^7.2.0",
    "eslint-config-ash-nazg": "^22.3.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-formatter-badger": "^0.9.0",
    "eslint-plugin-array-func": "^3.1.6",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-expect-keywords": "^2.0.1",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-compat": "^3.7.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^27.0.4",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-no-unsanitized": "^3.1.1",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^20.1.0",
    "esm": "^3.2.25",
    "license-badger": "^0.17.0",
    "mocha": "^7.2.0",
    "mocha-badge-generator": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "typescript": "^3.9.5"
  }
}
