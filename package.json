{
  "name": "es-file-traverse",
  "version": "0.1.0",
  "description": "Facilities for traversing JavaScript files",
  "author": "Brett Zamir",
  "contributors": [],
  "scripts": {
    "build-cli-svg": "clp --format=svg --config=src/optionDefinitions.js doc-includes/cli.svg",
    "build-cli": "npm run build-cli-svg",
    "cli": "node ./bin/cli.js",
    "help": "node ./bin/cli.js --help",
    "version": "node ./bin/cli.js --version",
    "cli-example": "npm run cli -- --file=./test/fixtures/main.js",
    "eslint": "eslint --ext=js,md,html .",
    "lint": "npm run eslint",
    "coverage-badge": "coveradge doc-includes/coverage-badge.svg",
    "test": "echo 'Removing esm cache which seems to blank out istanbul coverage when breaking out early' && rm -Rf node_modules/.cache && nyc mocha --reporter=mocha-badge-generator --reporter-option badge_output=doc-includes/tests-badge.svg --require=esm --require=chai/register-expect && npm run coverage-badge"
  },
  "engines": {
    "node": ">=10.4.0"
  },
  "nyc": {
    "reporter": [
      "json-summary",
      "text"
    ]
  },
  "repository": "https://github.com/brettz9/es-file-traverse",
  "license": "MIT",
  "keywords": [
    "eslint",
    "files"
  ],
  "main": "./src/index.js",
  "bin": {
    "es-file-traverse": "./bin/cli.js"
  },
  "homepage": "https://github.com/brettz9/es-file-traverse",
  "bugs": "https://github.com/brettz9/es-file-traverse/issues",
  "dependencies": {
    "babel-eslint": "^10.1.0",
    "command-line-basics": "^0.8.0",
    "eslint": "^6.8.0",
    "file-fetch": "^1.4.1",
    "resolve": "^1.15.1"
  },
  "devDependencies": {
    "@mysticatea/eslint-plugin": "^13.0.0",
    "chai": "^4.2.0",
    "command-line-publish": "^0.7.0",
    "coveradge": "^0.3.0",
    "eslint-config-ash-nazg": "^17.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-array-func": "^3.1.4",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-compat": "^3.5.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsdoc": "^22.0.0",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-no-unsanitized": "^3.0.2",
    "eslint-plugin-no-use-extend-native": "^0.4.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-unicorn": "^16.1.1",
    "esm": "^3.2.25",
    "license-badger": "^0.11.1",
    "mocha": "^7.1.0",
    "mocha-badge-generator": "^0.5.0",
    "mocha-multi-reporters": "git+https://github.com/brettz9/mocha-multi-reporters.git#missing-reporter",
    "nyc": "^15.0.0",
    "typescript": "^3.8.3"
  }
}
